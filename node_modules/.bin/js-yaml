#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/pguerry/GITHUB/pytheas_devtest/pytheas-db/node_modules/js-yaml/bin/node_modules:/home/pguerry/GITHUB/pytheas_devtest/pytheas-db/node_modules/js-yaml/node_modules:/home/pguerry/GITHUB/pytheas_devtest/pytheas-db/node_modules:/home/pguerry/GITHUB/pytheas_devtest/node_modules:/home/pguerry/GITHUB/node_modules:/home/pguerry/node_modules:/home/node_modules:/node_modules:/home/pguerry/GITHUB/pytheas_devtest/pytheas-db/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/pguerry/GITHUB/pytheas_devtest/pytheas-db/node_modules/js-yaml/bin/node_modules:/home/pguerry/GITHUB/pytheas_devtest/pytheas-db/node_modules/js-yaml/node_modules:/home/pguerry/GITHUB/pytheas_devtest/pytheas-db/node_modules:/home/pguerry/GITHUB/pytheas_devtest/node_modules:/home/pguerry/GITHUB/node_modules:/home/pguerry/node_modules:/home/node_modules:/node_modules:/home/pguerry/GITHUB/pytheas_devtest/pytheas-db/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../js-yaml/bin/js-yaml.js" "$@"
else
  exec node  "$basedir/../js-yaml/bin/js-yaml.js" "$@"
fi
